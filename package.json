{
  "name": "jmp",
  "displayName": "jmp",
  "description": "Motion extension to jump to anywhere in visible editor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jaimeloeuf/jmp.git"
  },
  "bugs": {
    "url": "https://github.com/Jaimeloeuf/jmp/issues"
  },
  "author": "JJ",
  "publisher": "JJ",
  "license": "MIT",
  "icon": "logo.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "shortcuts",
    "goto",
    "jump",
    "seek",
    "leap",
    "easymotion",
    "motion"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jmp.jump",
        "title": "Jump to a location"
      }
    ],
    "keybindings": [
      {
        "key": "Cmd+e",
        "command": "jmp.jump",
        "when": "editorTextFocus && isMac"
      },
      {
        "key": "Alt+e",
        "command": "jmp.jump",
        "when": "editorTextFocus && (isWindows || isLinux)"
      }
    ],
    "configuration": {
      "title": "JMP",
      "properties": {
        "jmp.jumpOnFound": {
          "description": "Jump to target immediately if the first 2 characters is unique.",
          "type": "boolean",
          "default": false
        },
        "jmp.centerOnJump": {
          "description": "Move line of jump target to center of screen.",
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}